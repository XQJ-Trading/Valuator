# Google Gemini API Configuration
GOOGLE_API_KEY=your-google-api-here

# Perplexity API Configuration
PPLX_API_KEY=your-pplx-api-here

# Agent Configuration
AGENT_NAME=MyAgent
AGENT_VERSION=1.5.0
# (default model setting)
AGENT_MODEL=gemini-3-pro-preview

# Supported Models Configuration (comma-separated list)
SUPPORTED_MODELS=gemini-3-pro-preview

# Model Configuration
TEMPERATURE=1.0
MAX_TOKENS=30000
TOP_P=0.95
TOP_K=64

# Thinking Level 설정 (선택사항)
GEMINI_THINKING_LEVEL=high

# ReAct Engine Configuration
REACT_MAX_RETRIES=3
REACT_MAX_THOUGHT_CYCLES=100
REACT_PLANNING_INTERVAL=3

# Tool Configuration
CODE_EXECUTION_TIMEOUT=30

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/ai_agent.log

# low-level Gemini LLM query log configuration
GEMINI_LOW_LEVEL_REQUEST_LOGGING=true

# MongoDB Configuration (Optional - for ReAct session logging)
MONGODB_ENABLED=false
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=ai_agent
MONGODB_COLLECTION=react_sessions